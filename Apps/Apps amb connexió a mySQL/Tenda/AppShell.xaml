<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="Tenda.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:Tenda"
    Shell.FlyoutBehavior="Flyout"
    Title="Tenda">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:PaginaSecundaria}"
        Route="Home" />
    <ShellContent
        Title="Camisetes"
        ContentTemplate="{DataTemplate local:Camisetes}"
        Route="camisetes" />
    <ShellContent
        Title="Pantalons"
        ContentTemplate="{DataTemplate local:Pantalons}"
        Route="pantalons" />

</Shell>
